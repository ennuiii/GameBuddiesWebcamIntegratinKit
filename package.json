{
  "name": "@gamebuddies/webcam-kit",
  "version": "1.0.0",
  "description": "Complete WebRTC video chat system with AI enhancements - Virtual backgrounds, face avatars, and noise suppression",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "public",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "webrtc",
    "video-chat",
    "react",
    "mediapipe",
    "virtual-background",
    "face-tracking",
    "noise-suppression",
    "webcam",
    "video-conferencing"
  ],
  "author": "GameBuddies Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ennuiii/GameBuddiesWebcamIntegratinKit.git"
  },
  "bugs": {
    "url": "https://github.com/ennuiii/GameBuddiesWebcamIntegratinKit/issues"
  },
  "homepage": "https://github.com/ennuiii/GameBuddiesWebcamIntegratinKit#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "socket.io-client": "^4.7.0"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.15",
    "three": "^0.160.0",
    "lucide-react": "^0.300.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  },
  "scripts": {
    "build": "tsc && vite build",
    "prepublishOnly": "npm run build",
    "setup-assets": "node scripts/setup-assets.js"
  }
}
